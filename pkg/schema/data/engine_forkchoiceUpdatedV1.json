{
  "summary": "Updates the forkchoice state",
  "description": "",
  "name": "engine_forkchoiceUpdatedV1",
  "params": [
    {
      "summary": "",
      "description": "Forkchoice state",
      "name": "forkchoice_state",
      "schema": {
        "$ref": "#/$defs/ForkchoiceStateV1"
      },
      "required": true,
      "deprecated": false
    },
    {
      "summary": "",
      "description": "Payload attributes",
      "name": "payload_attributes",
      "schema": {
        "$ref": "#/$defs/PayloadAttributesV1"
      },
      "required": false,
      "deprecated": false
    }
  ],
  "defs": {
    "ForkchoiceStateV1": {
      "properties": {
        "finalizedBlockHash": {
          "$ref": "#/$defs/hash32",
          "title": "Finalized block hash"
        },
        "headBlockHash": {
          "$ref": "#/$defs/hash32",
          "title": "Head block hash"
        },
        "safeBlockHash": {
          "$ref": "#/$defs/hash32",
          "title": "Safe block hash"
        }
