{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/selesy/ethereum-mcp/engine_newPayloadV3.json",
  "$defs": {
    "ExecutionPayloadV3": {
      "properties": {
        "baseFeePerGas": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/baseFeePerGas"
        },
        "blobGasUsed": {
          "$ref": "#/$defs/uint64",
          "title": "Blob gas used"
        },
        "blockHash": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/blockHash"
        },
        "blockNumber": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/blockNumber"
        },
        "excessBlobGas": {
          "$ref": "#/$defs/uint64",
          "title": "Excess blob gas"
        },
        "extraData": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/extraData"
        },
        "feeRecipient": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/feeRecipient"
        },
        "gasLimit": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/gasLimit"
        },
        "gasUsed": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/gasUsed"
        },
        "logsBloom": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/logsBloom"
        },
        "parentHash": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/parentHash"
        },
        "prevRandao": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/prevRandao"
        },
        "receiptsRoot": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/receiptsRoot"
        },
        "stateRoot": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/stateRoot"
        },
        "timestamp": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/timestamp"
        },
        "transactions": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/transactions"
        },
        "withdrawals": {
          "$ref": "#/$defs/ExecutionPayloadV2/properties/withdrawals"
        }
      },
      "type": "object",
      "required": [
        "parentHash",
        "feeRecipient",
        "stateRoot",
        "receiptsRoot",
        "logsBloom",
        "prevRandao",
        "blockNumber",
        "gasLimit",
        "gasUsed",
        "timestamp",
        "extraData",
        "baseFeePerGas",
        "blockHash",
        "transactions",
        "withdrawals",
        "blobGasUsed",
        "excessBlobGas"
      ],
      "title": "Execution payload object V3"
    },
    "baseFeePerGas": null,
    "blockHash": null,
    "blockNumber": null,
    "extraData": null,
    "feeRecipient": null,
    "gasLimit": null,
    "gasUsed": null,
    "hash32": {
      "type": "string",
      "pattern": "^0x[0-9a-f]{64}$",
      "title": "32 byte hex value"
    },
    "logsBloom": null,
    "parentHash": null,
    "prevRandao": null,
    "receiptsRoot": null,
    "stateRoot": null,
    "timestamp": null,
    "transactions": null,
    "uint64": {
      "type": "string",
      "pattern": "^0x(0|[1-9a-f][0-9a-f]{0,15})$",
      "title": "hex encoded 64 bit unsigned integer"
    },
    "withdrawals": null
  },
  "properties": {
    "execution_payload": {
      "$ref": "#/$defs/ExecutionPayloadV3",
      "title": "execution_payload",
      "description": "Execution payload"
    },
    "expected_blob_versioned_hashes": {
      "items": {
        "$ref": "#/$defs/hash32"
      },
      "type": "array",
      "title": "expected_blob_versioned_hashes",
      "description": "Expected blob versioned hashes"
    },
    "root_of_the_parent_beacon_block": {
      "$ref": "#/$defs/hash32",
      "title": "root_of_the_parent_beacon_block",
      "description": "Root of the parent beacon block"
    }
  },
  "type": "object",
  "required": [
    "execution_payload",
    "expected_blob_versioned_hashes",
    "root_of_the_parent_beacon_block"
  ],
  "title": "engine_newPayloadV3",
  "description": "Runs execution payload validation"
}
