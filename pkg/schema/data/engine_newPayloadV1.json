{
  "summary": "Runs execution payload validation",
  "description": "",
  "name": "engine_newPayloadV1",
  "params": [
    {
      "summary": "",
      "description": "Execution payload",
      "name": "execution_payload",
      "schema": {
        "$ref": "#/$defs/ExecutionPayloadV1"
      },
      "required": true,
      "deprecated": false
    }
  ],
  "defs": {
    "ExecutionPayloadV1": {
      "properties": {
        "baseFeePerGas": {
          "$ref": "#/$defs/uint256",
          "title": "Base fee per gas"
        },
        "blockHash": {
          "$ref": "#/$defs/hash32",
          "title": "Block hash"
        },
        "blockNumber": {
          "$ref": "#/$defs/uint64",
          "title": "Block number"
        },
        "extraData": {
          "$ref": "#/$defs/bytesMax32",
          "title": "Extra data"
        },
        "feeRecipient": {
          "$ref": "#/$defs/address",
          "title": "Recipient of transaction priority fees"
        },
        "gasLimit": {
          "$ref": "#/$defs/uint64",
          "title": "Gas limit"
        },
        "gasUsed": {
          "$ref": "#/$defs/uint64",
          "title": "Gas used"
        },
        "logsBloom": {
          "$ref": "#/$defs/bytes256",
          "title": "Bloom filter"
        },
        "parentHash": {
          "$ref": "#/$defs/hash32",
          "title": "Parent block hash"
        },
        "prevRandao": {
          "$ref": "#/$defs/bytes32",
          "title": "Previous randao value"
        },
        "receiptsRoot": {
          "$ref": "#/$defs/hash32",
          "title": "Receipts root"
        },
        "stateRoot": {
          "$ref": "#/$defs/hash32",
          "title": "State root"
        },
        "timestamp": {
          "$ref": "#/$defs/uint64",
          "title": "Timestamp"
        },
        "transactions": {
          "items": {
            "$ref": "#/$defs/bytes"
          },
          "type": "array",
          "title": "Transactions"
        }
      },
      "type": "object",
      "required": [
        "parentHash",
        "feeRecipient",
        "stateRoot",
        "receiptsRoot",
        "logsBloom",
        "prevRandao",
        "blockNumber",
        "gasLimit",
        "gasUsed",
        "timestamp",
        "extraData",
        "baseFeePerGas",
        "blockHash",
        "transactions"
      ],
      "title": "Execution payload object V1"
    },
    "address": {
      "type": "string",
      "pattern": "^0x[0-9a-fA-F]{40}$",
      "title": "hex encoded address"
    },
    "bytes": {
      "type": "string",
      "pattern": "^0x[0-9a-f]*$",
      "title": "hex encoded bytes"
    },
    "bytes256": {
      "type": "string",
      "pattern": "^0x[0-9a-f]{512}$",
      "title": "256 hex encoded bytes"
    },
    "bytes32": {
      "type": "string",
      "pattern": "^0x[0-9a-f]{64}$",
      "title": "32 hex encoded bytes"
    },
    "bytesMax32": {
      "type": "string",
      "pattern": "^0x[0-9a-f]{0,64}$",
      "title": "32 hex encoded bytes"
    },
    "hash32": {
      "type": "string",
      "pattern": "^0x[0-9a-f]{64}$",
      "title": "32 byte hex value"
    },
    "uint256": {
      "type": "string",
      "pattern": "^0x(0|[1-9a-f][0-9a-f]{0,63})$",
      "title": "hex encoded 256 bit unsigned integer"
    },
    "uint64": {
      "type": "string",
      "pattern": "^0x(0|[1-9a-f][0-9a-f]{0,15})$",
      "title": "hex encoded 64 bit unsigned integer"
    }
  }
}
