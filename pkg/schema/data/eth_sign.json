{
  "summary": "Returns an EIP-191 signature over the provided data.",
  "description": "",
  "name": "eth_sign",
  "params": [
    {
      "summary": "",
      "description": "Address",
      "name": "address",
      "schema": {
        "$ref": "#/$defs/address"
      },
      "required": true,
      "deprecated": false
    },
    {
      "summary": "",
      "description": "Message",
      "name": "message",
      "schema": {
        "$ref": "#/$defs/bytes"
      },
      "required": true,
      "deprecated": false
    }
  ],
  "defs": {
    "address": {
      "type": "string",
      "pattern": "^0x[0-9a-fA-F]{40}$",
      "title": "hex encoded address"
    },
    "bytes": {
      "type": "string",
      "pattern": "^0x[0-9a-f]*$",
      "title": "hex encoded bytes"
    }
  }
}
