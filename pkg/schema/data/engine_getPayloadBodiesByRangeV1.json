{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/selesy/ethereum-mcp/engine_getPayloadBodiesByRangeV1.json",
  "$defs": {
    "uint64": {
      "type": "string",
      "pattern": "^0x(0|[1-9a-f][0-9a-f]{0,15})$",
      "title": "hex encoded 64 bit unsigned integer"
    }
  },
  "properties": {
    "starting_block_number": {
      "$ref": "#/$defs/uint64",
      "title": "starting_block_number",
      "description": "Starting block number"
    },
    "number_of_blocks_to_return": {
      "$ref": "#/$defs/uint64",
      "title": "number_of_blocks_to_return",
      "description": "Number of blocks to return"
    }
  },
  "type": "object",
  "required": [
    "starting_block_number",
    "number_of_blocks_to_return"
  ],
  "title": "engine_getPayloadBodiesByRangeV1",
  "description": "Given a range of block numbers returns bodies of the corresponding execution payloads"
