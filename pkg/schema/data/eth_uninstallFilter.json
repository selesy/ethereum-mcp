{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/selesy/ethereum-mcp/eth_uninstallFilter.json",
  "$defs": {
    "uint": {
      "type": "string",
      "pattern": "^0x(0|[1-9a-f][0-9a-f]*)$",
      "title": "hex encoded unsigned integer"
    }
  },
  "properties": {
    "filter_identifier": {
      "$ref": "#/$defs/uint",
      "title": "filter_identifier",
      "description": "Filter identifier"
    }
  },
  "type": "object",
  "required": [
    "filter_identifier"
  ],
  "title": "eth_uninstallFilter",
  "description": "Uninstalls a filter with given id."
}
