{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/selesy/ethereum-mcp/engine_getBlobsV1.json",
  "$defs": {
    "hash32": {
      "type": "string",
      "pattern": "^0x[0-9a-f]{64}$",
      "title": "32 byte hex value"
    }
  },
  "properties": {
    "blob_versioned_hashes": {
      "items": {
        "$ref": "#/$defs/hash32"
      },
      "type": "array",
      "title": "blob_versioned_hashes",
      "description": "Blob versioned hashes"
    }
  },
  "type": "object",
  "required": [
    "blob_versioned_hashes"
  ],
  "title": "engine_getBlobsV1",
  "description": "Fetches blobs from the blob pool"
}
